<!DOCTYPE kpartgui>
<effect tag="typewriter" id="typewriter" type="video" unique="1">
    <name>TypeWriter</name>
    <description>Typewriter effect v0.2.0</description>
    <author>Rafal Lalik</author>
    <parameter type="int" name="framestep" min="1" max="240" default="25">
